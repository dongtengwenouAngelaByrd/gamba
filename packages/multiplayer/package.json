    "clean": "rm -rf .turbo node_modules dist"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/web3.js": "^1.98.2",
    "@solana/spl-token": "^0.4.13"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "solana",
    "anchor",
    "gamba",
    "multiplayer"
