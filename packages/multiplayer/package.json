{
  "name": "@gamba-labs/multiplayer-sdk",
  "version": "2.0.0",
  "private": false,
  "description": "Gamba Multiplayer on-chain game helper layer (Anchor 0.31.1)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "idl/**"
  ],
  "sideEffects": false,
  "scripts": {
